{
  "name": "@ideas-hub/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.node.json && tsc --noEmit --project ./tsconfig.app.json",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "prettify": "prettier --log-level=warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' --ignore-path ../.prettierignore"
  },
  "dependencies": {
    "@ideas-hub/backend": "workspace:*",
    "@tanstack/react-query": "^5.75.7",
    "@trpc/client": "^11.1.2",
    "@trpc/react-query": "^11.1.2",
    "include-media": "^2.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "reset-css": "^5.0.2",
    "sass": "^1.89.2"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "prettier": "^3.5.3",
    "vite": "^6.2.0"
  }
}
